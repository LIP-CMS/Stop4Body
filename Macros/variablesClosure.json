{
  "cuts":[
    {
      "name":"Preselection",
      "expression":"(HT > 200) && (Met > 200) && (nGoodEl+nGoodMu <= 2)",
      "latex":"Preselection"
    },
    {
      "name":"PreselectionMVA_allLep",
      "expression":"(HT > 200) && (Met > 280) && (Jet1Pt > 110) && (nGoodEl+nGoodMu <= 2) && (HLT_Mu == 1)",
      "latex":"Preselection MVA"
    },
    {
      "name":"PreselectionMVA_lep30",
      "expression":"(HT > 200) && (Met > 280) && (Jet1Pt > 110) && (nGoodEl+nGoodMu <= 2) && (LepPt < 30) && (HLT_Mu == 1)",
      "latex":"Preselection MVA ($p_T(l) < 30$)"
    },
    {
      "name":"SelectionMVA_allLep",
      "expression":"(HT > 200) && (Met > 280) && (Jet1Pt > 110) && (nGoodEl+nGoodMu <= 2) && (BDT > 0.4) && (HLT_Mu == 1)",
      "latex":"Selection MVA"
    },
    {
      "name":"SelectionMVA_lep30",
      "expression":"(HT > 200) && (Met > 280) && (Jet1Pt > 110) && (nGoodEl+nGoodMu <= 2) && (LepPt < 30) && (BDT > 0.4) && (HLT_Mu == 1)",
      "latex":"Selection MVA ($p_T(l) < 30$)"
    },
    {
      "name":"SelectionMVA_lep30_wjets",
      "expression":"(HT > 200) && (Met > 280) && (Jet1Pt > 110) && (nGoodEl+nGoodMu <= 2) && (LepPt < 30) && (BDT > 0.4) && (HLT_Mu == 1) && (NbLoose == 0)",
      "latex":"Selection MVA ($p_T(l) < 30$, wjets)"
    },
    {
      "name":"SelectionMVA_lep30_ttbar",
      "expression":"(HT > 200) && (Met > 280) && (Jet1Pt > 110) && (nGoodEl+nGoodMu <= 2) && (LepPt < 30) && (BDT > 0.4) && (HLT_Mu == 1) && (NbTight > 0)",
      "latex":"Selection MVA ($p_T(l) < 30$, ttbar)"
    }
  ],
  "variables":[
    {
      "expression":"nVert",
      "min":0.0,
      "max":50.0,
      "bins":50
    },
    {
      "expression":"BDT",
      "min":-1.0,
      "max":1.0,
      "bins":20
    }
  ]
}
